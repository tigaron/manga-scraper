    "definitions": {
        "v1Binding.PostProviderRequest": {
            "type": "object",
            "required": [
                "host",
                "is_active",
                "list_path",
                "name",
                "scheme",
                "slug"
            ],
            "properties": {
                "host": {
                    "type": "string",
                    "example": "asuratoon.com"
                },
                "is_active": {
                    "type": "boolean",
                    "example": true
                },
                "list_path": {
                    "type": "string",
                    "example": "/manga/list-mode/"
                },
                "name": {
                    "type": "string",
                    "example": "Asura Scans"
                },
                "scheme": {
                    "type": "string",
                    "example": "https://"
                },
                "slug": {
                    "type": "string",
                    "example": "asura"
                }
            }
        },
        "v1Binding.PostScrapeChapterList": {
            "type": "object",
            "required": [
                "provider",
                "series"
            ],
            "properties": {
                "provider": {
                    "type": "string",
                    "example": "asura"
                },
                "series": {
                    "type": "string",
                    "example": "reincarnator"
                }
            }
        },
        "v1Binding.PostScrapeSeriesList": {
            "type": "object",
            "required": [
